{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gallatin/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gallatin/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "monorepo": true,
  "root": "apps/gallatin",
  "projects": {
    "gallatin": {
      "type": "application",
      "root": "apps/gallatin",
      "entryFile": "main",
      "sourceRoot": "apps/gallatin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gallatin/tsconfig.app.json"
      }
    },
    "nashville": {
      "type": "application",
      "root": "apps/nashville",
      "entryFile": "main",
      "sourceRoot": "apps/nashville/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nashville/tsconfig.app.json"
      }
    },
    "ashland": {
      "type": "application",
      "root": "apps/ashland",
      "entryFile": "main",
      "sourceRoot": "apps/ashland/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ashland/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}